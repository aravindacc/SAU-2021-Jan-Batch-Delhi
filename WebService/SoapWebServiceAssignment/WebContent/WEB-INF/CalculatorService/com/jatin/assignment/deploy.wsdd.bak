<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from CalculatorService WSDL service -->

  <service name="Calculator" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://assignment.jatin.com"/>
      <parameter name="wsdlServiceElement" value="CalculatorService"/>
      <parameter name="schemaQualified" value="http://assignment.jatin.com"/>
      <parameter name="wsdlServicePort" value="Calculator"/>
      <parameter name="className" value="com.jatin.assignment.CalculatorSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="Calculator"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="sinOf" qname="operNS:sinOf" xmlns:operNS="http://assignment.jatin.com" returnQName="retNS:sinOfReturn" xmlns:retNS="http://assignment.jatin.com" returnType="rtns:double" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:value" xmlns:pns="http://assignment.jatin.com" type="tns:double" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="cosOf" qname="operNS:cosOf" xmlns:operNS="http://assignment.jatin.com" returnQName="retNS:cosOfReturn" xmlns:retNS="http://assignment.jatin.com" returnType="rtns:double" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:value" xmlns:pns="http://assignment.jatin.com" type="tns:double" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="factorial" qname="operNS:factorial" xmlns:operNS="http://assignment.jatin.com" returnQName="retNS:factorialReturn" xmlns:retNS="http://assignment.jatin.com" returnType="rtns:int" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:number" xmlns:pns="http://assignment.jatin.com" type="tns:double" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="tanOf" qname="operNS:tanOf" xmlns:operNS="http://assignment.jatin.com" returnQName="retNS:tanOfReturn" xmlns:retNS="http://assignment.jatin.com" returnType="rtns:double" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:value" xmlns:pns="http://assignment.jatin.com" type="tns:double" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="logBase" qname="operNS:logBase" xmlns:operNS="http://assignment.jatin.com" returnQName="retNS:logBaseReturn" xmlns:retNS="http://assignment.jatin.com" returnType="rtns:double" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:value" xmlns:pns="http://assignment.jatin.com" type="tns:double" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:base" xmlns:pns="http://assignment.jatin.com" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="log2" qname="operNS:log2" xmlns:operNS="http://assignment.jatin.com" returnQName="retNS:log2Return" xmlns:retNS="http://assignment.jatin.com" returnType="rtns:double" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:value" xmlns:pns="http://assignment.jatin.com" type="tns:double" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="logBase factorial tanOf cosOf sinOf log2"/>

  </service>
</deployment>
